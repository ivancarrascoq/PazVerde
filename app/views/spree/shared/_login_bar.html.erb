<!--app/views/spree/shared/_login_bar.html.erb-->
<% if spree_current_user %>
  <li id="my-accnt"><%= link_to Spree.t(:my_account), spree.account_path %></li>
  <li id="logout"><%= link_to Spree.t(:logout), spree.logout_path %></li>
<% else %>
  <li id="link-to-login"><%= link_to Spree.t(:login), spree.login_path %></li>
<% end %>

